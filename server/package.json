{
    "name": "launchsin-server",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "check": "tsc --noEmit",
        "start": "node dist/index.js",
        "test": "npx jest --coverage --coverageThreshold='{\"global\":{\"lines\":15,\"statements\":15,\"functions\":10,\"branches\":8}}'",
        "lint": "eslint src/**/*.ts"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/tests/**/*.test.ts"
        ],
        "moduleNameMapper": {
            "^(\\.{1,2}/.*)\\.js$": "$1"
        },
        "coverageThreshold": {
            "global": {
                "lines": 15,
                "statements": 15,
                "functions": 10,
                "branches": 8
            }
        }
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.89.0",
        "axios": "^1.6.5",
        "bullmq": "^5.66.4",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "ioredis": "^5.9.0",
        "openai": "^4.24.1",
        "prom-client": "^15.1.3",
        "puppeteer": "^24.34.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.10.6",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^9.0.7",
        "jest": "^30.2.0",
        "supertest": "^7.2.2",
        "ts-jest": "^29.4.6",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}